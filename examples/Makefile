LOCAL_LIBRARIES = -lm

CCOPTIONS = -O3

CXXDEBUGFLAGS =

CDEBUGFLAGS =

F77 = g77

CC = gcc

CCLINK = $(CC)

TOP_INCLUDES =  -I. 		-I$(LOCAL_INCDIR)

EXTRA_LDOPTIONS = -I. 		-I$(LOCAL_INCDIR)

SRCS =  phase_screen_check.c 	turbulent_phase_screen.c 	seed_random.c 	char_copy.c 	gauss_dev.c 	random_val1.c

OBJS =  phase_screen_check.o 	turbulent_phase_screen.o 	seed_random.o 	char_copy.o 	gauss_dev.o 	random_val1.o

        PROGRAM = phase_screen_check

all:: phase_screen_check

phase_screen_check: $(OBJS) $(DEPLIBS)
	$(RM) $@
	$(CCLINK) -o $@ $(LDOPTIONS) $(OBJS) $(LOCAL_LIBRARIES) $(LDLIBS)  $(EXTRA_LOAD_FLAGS)

install:: phase_screen_check
	@if [ -d $(DESTDIR)$(BINDIR) ]; then \
		set +x; \
	else \
		if [ -h $(DESTDIR)$(BINDIR) ]; then \
			(set -x; rm -f $(DESTDIR)$(BINDIR)); \
		fi; \
		(set -x; $(MKDIRHIER) $(DESTDIR)$(BINDIR)); \
	fi
	$(INSTALL) $(INSTALLFLAGS) $(INSTPGMFLAGS)  phase_screen_check $(DESTDIR)$(BINDIR)/phase_screen_check

all:: phase_screen_check.$(MANSUFFIX).html

phase_screen_check.$(MANSUFFIX).html: phase_screen_check.$(MANNEWSUFFIX)
	$(RM) phase_screen_check.$(MANSUFFIX).html phase_screen_check.$(MANSUFFIX)-html
	$(RMAN) $(RMANOPTIONS) < phase_screen_check.$(MANNEWSUFFIX) \
	  > phase_screen_check.$(MANSUFFIX)-html && $(MV) phase_screen_check.$(MANSUFFIX)-html $@

install.man:: phase_screen_check.$(MANSUFFIX).html
	@if [ -d $(DESTDIR)$(DOCHTMLDIR) ]; then \
		set +x; \
	else \
		if [ -h $(DESTDIR)$(DOCHTMLDIR) ]; then \
			(set -x; rm -f $(DESTDIR)$(DOCHTMLDIR)); \
		fi; \
		(set -x; $(MKDIRHIER) $(DESTDIR)$(DOCHTMLDIR)); \
	fi
	@(SUF=`expr $(MANSUFFIX) \: '\(.\)'`; \
	 set -x; \
	 $(INSTALL) $(INSTALLFLAGS) $(INSTMANFLAGS) phase_screen_check.$(MANSUFFIX).html $(DESTDIR)$(DOCHTMLDIR)/phase_screen_check.$$SUF.html)

cleandir::
	$(RM) phase_screen_check.$(MANSUFFIX).html phase_screen_check.$(MANSUFFIX)-html

all:: phase_screen_check.$(MANNEWSUFFIX)

phase_screen_check.$(MANNEWSUFFIX): phase_screen_check.$(MANSRCSUFFIX)
	$(RM) $@
	cd `dirname phase_screen_check` && \
		$(LN) `basename phase_screen_check.$(MANSRCSUFFIX)` `basename $@`

cleandir::
	$(RM) phase_screen_check.$(MANNEWSUFFIX)

install.man:: phase_screen_check.$(MANNEWSUFFIX)
	@if [ -d $(DESTDIR)$(MANDIR) ]; then \
		set +x; \
	else \
		if [ -h $(DESTDIR)$(MANDIR) ]; then \
			(set -x; rm -f $(DESTDIR)$(MANDIR)); \
		fi; \
		(set -x; $(MKDIRHIER) $(DESTDIR)$(MANDIR)); \
	fi
	$(INSTALL) $(INSTALLFLAGS) $(INSTMANFLAGS) phase_screen_check.$(MANNEWSUFFIX) $(DESTDIR)$(MANDIR)/phase_screen_check.$(MANSUFFIX)

